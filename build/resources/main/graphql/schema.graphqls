schema{
    query: Query
}
type Actor{
    actorId:ID!,
    firstName:String,
    lastName:String,
    dob:String,
    address:String,
    filmId:Int
    film:Film # film도 가져올 수 있도록 정의
}

type Film{
    filmId:ID!,
    name:String,
    regDt:String
}

type Query{
    getAllActors:[Actor],
    getActorById(id:Int):Actor
}
input AddressInput{
    actorId:Int,
    address:String
}
type Mutation{
    # 매개변수 id는 int타입, adress는 문자열 타입
    # 반환값은 Actor
    updateAddress(id:Int, address:String):Actor,
    ##
    updateAddressByInputObject(input:AddressInput):Actor
}